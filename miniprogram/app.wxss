/**app.wxss**/
@font-face {
  font-family: 'myfont';
  src:url("https://wyxh-1322159997.cos.ap-nanjing.myqcloud.com/myfont.ttf") format('truetype');
}

body {
  font-family: 'myfont', sans-serif;
}

.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 

button {
  background: initial;
}

button:focus{
  outline: 0;
}

button::after{
  border: none;
}

page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  font-family: Verdana, Geneva, Tahoma, sans-serif;
}