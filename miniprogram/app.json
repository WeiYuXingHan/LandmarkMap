{
  "pages": [
    "pages/index/index",
    "pages/landmark/landmark",
    "pages/aboutme/aboutme",
    "pages/route/route"
  ],
  "window": {
    "backgroundColor": "#F6F6F6",
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#F6F6F6",
    "navigationBarTitleText": "红旅金陵",
    "navigationBarTextStyle": "black",
    "enablePullDownRefresh": true
  },
  "tabBar": {
    "selectedColor": "#ea8010",
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "首页",
        "iconPath": "images/tab/home.png",
        "selectedIconPath": "images/tab/home-2.png"
      },
      {
        "pagePath": "pages/landmark/landmark",
        "text": "纪念碑",
        "iconPath": "images/tab/landmark.png",
        "selectedIconPath": "images/tab/landmark-2.png"
      },
      {
        "pagePath": "pages/route/route",
        "text": "地标",
        "iconPath": "images/tab/route.png",
        "selectedIconPath": "images/tab/route-2.png"
      },
      {
        "pagePath": "pages/aboutme/aboutme",
        "text": "红旅金陵",
        "iconPath": "images/tab/aboutme.png",
        "selectedIconPath": "images/tab/aboutme-2.png"
      }
    ]
  },
  "subpackages": [
    {
      "root": "pages/subpages",
      "pages": [
        "card/card",
        "items/items",
        "register/register",
        "search/search",
        "location/location",
        "help/help",
        "aboutus/aboutus",
        "sponsor/sponsor"
      ],
      "plugins": {
        "routePlan": {
          "version": "1.0.19",
          "provider": "wx50b5593e81dd937a"
        }
      }
    }
  ],
  "sitemapLocation": "sitemap.json",
  "lazyCodeLoading": "requiredComponents",
  "requiredBackgroundModes": ["location"],
  "permission": {
    "scope.userLocation": {
      "desc": "你的位置信息将用于小程序位置接口的效果展示"
    },
    "scope.camera": {
      "desc": "你的相机将用于扫描二维码"
    }
  },
  "useExtendedLib": {
    "kbone": true,
    "weui": true
  },
  "usingComponents": {
    "van-button": "@vant/weapp/button/index",
    "van-popup": "@vant/weapp/popup/index",
    "van-cell": "@vant/weapp/cell/index",
    "van-cell-group": "@vant/weapp/cell-group/index",
    "van-search": "@vant/weapp/search/index",
    "van-card": "@vant/weapp/card/index",
    "van-field": "@vant/weapp/field/index"
  }
}
